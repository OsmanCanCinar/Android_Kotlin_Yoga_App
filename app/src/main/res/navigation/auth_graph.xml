<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/auth_graph"
    app:startDestination="@id/greetFragment">

    <fragment
        android:id="@+id/greetFragment"
        android:name="com.osmancancinar.yogaapp.ui.authentication.GreetFragment"
        android:label="fragment_greet"
        tools:layout="@layout/fragment_greet" >
        <action
            android:id="@+id/action_greetFragment_to_signUpFragment"
            app:destination="@id/signUpFragment" />
    </fragment>
    <fragment
        android:id="@+id/signInFragment"
        android:name="com.osmancancinar.yogaapp.ui.authentication.SignInFragment"
        android:label="fragment_sign_in"
        tools:layout="@layout/fragment_sign_in" >
        <action
            android:id="@+id/action_signInFragment_to_signUpFragment3"
            app:destination="@id/signUpFragment" />
    </fragment>
    <fragment
        android:id="@+id/signUpFragment"
        android:name="com.osmancancinar.yogaapp.ui.authentication.SignUpFragment"
        android:label="fragment_sign_up"
        tools:layout="@layout/fragment_sign_up" >
        <action
            android:id="@+id/action_signUpFragment_to_signInFragment3"
            app:destination="@id/signInFragment" />
    </fragment><action android:id="@+id/action_global_greetFragment" app:destination="@id/greetFragment"/>
</navigation>