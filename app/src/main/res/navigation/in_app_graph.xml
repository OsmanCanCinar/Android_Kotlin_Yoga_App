<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/in_app_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.osmancancinar.yogaapp.view.ui.home.HomeFragment"
        android:label="Home"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeFragment_to_meditationFragment2"
            app:destination="@id/meditationFragment" />
        <action
            android:id="@+id/action_homeFragment_to_yogaFragment2"
            app:destination="@id/yogaFragment" />
        <action
            android:id="@+id/action_homeFragment_to_blogFragment2"
            app:destination="@id/blogFragment" />
        <action
            android:id="@+id/action_homeFragment_to_profileFragment2"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_homeFragment_to_settingsFragment2"
            app:destination="@id/settingsFragment" />
        <action
            android:id="@+id/action_homeFragment_to_settingsFragment"
            app:destination="@id/settingsFragment" />
    </fragment>
    <fragment
        android:id="@+id/blogFragment"
        android:name="com.osmancancinar.yogaapp.view.ui.home.BlogFragment"
        android:label="Blog"
        tools:layout="@layout/fragment_blog">
        <action
            android:id="@+id/action_blogFragment_to_homeFragment2"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/meditationFragment"
        android:name="com.osmancancinar.yogaapp.view.ui.home.MeditationsListFragment"
        android:label="Meditation"
        tools:layout="@layout/fragment_meditation">
        <action
            android:id="@+id/action_meditationFragment_to_homeFragment2"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/yogaFragment"
        android:name="com.osmancancinar.yogaapp.view.ui.home.yoga.YogaFragment"
        android:label="Yoga"
        tools:layout="@layout/fragment_yoga">
        <action
            android:id="@+id/action_yogaFragment_to_homeFragment2"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.osmancancinar.yogaapp.view.ui.home.ProfileFragment"
        android:label="Profile"
        tools:layout="@layout/fragment_profile">
        <action
            android:id="@+id/action_profileFragment_to_homeFragment2"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.osmancancinar.yogaapp.view.ui.home.SettingsFragment"
        android:label="Settings"
        tools:layout="@layout/fragment_settings">
        <action
            android:id="@+id/action_settingsFragment2_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
</navigation>